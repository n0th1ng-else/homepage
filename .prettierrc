{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": "*.svg",
			"options": {
				"parser": "html"
			}
		}
	],
	"svelteStrictMode": true
}
