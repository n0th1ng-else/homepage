{
	"name": "my-github-page",
	"version": "0.0.0-semantically-released",
	"description": "My home page",
	"dependencies": {
		"date-fns": "2.29.3",
		"highlight.js": "11.7.0",
		"js-cookie": "^3.0.1",
		"marked": "4.2.12",
		"parse-md": "3.0.3",
		"zod": "^3.20.6"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "1.2.0",
		"@sveltejs/adapter-static": "2.0.1",
		"@sveltejs/kit": "1.15.2",
		"@tsconfig/svelte": "3.0.0",
		"@types/js-cookie": "^3.0.2",
		"@types/marked": "4.0.8",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"cross-spawn": "7.0.3",
		"dotenv-cli": "^7.0.0",
		"eslint": "8.34.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"lint-staged": "13.1.2",
		"metascraper": "5.33.8",
		"metascraper-author": "5.33.7",
		"metascraper-date": "5.33.7",
		"metascraper-description": "5.33.7",
		"metascraper-image": "5.33.7",
		"metascraper-logo": "5.33.7",
		"metascraper-publisher": "5.33.7",
		"metascraper-title": "5.33.7",
		"metascraper-url": "5.33.7",
		"node-fetch": "3.3.0",
		"postcss": "8.4.21",
		"prettier": "2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"sass": "1.58.1",
		"semantic-release": "20.1.0",
		"slug": "8.2.2",
		"svelte": "3.55.1",
		"svelte-check": "3.0.3",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"vite": "4.1.1"
	},
	"scripts": {
		"meta": "node links.js",
		"meta:dev": "dotenv npm run meta",
		"version": "node version.js",
		"format": "prettier --plugin-search-dir --write",
		"format:check": "prettier --plugin-search-dir --check .",
		"lint": "eslint",
		"lint:all": "eslint .",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv vite dev",
		"preview": "vite preview",
		"start": "node dist",
		"prepare": "svelte-kit sync"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"typescript",
		"webpack",
		"sass"
	],
	"author": "Sergey Nikitin <srg.post@gmail.com>",
	"license": "MIT",
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{md,yml,scss,css,svelte}": [
			"npm run format"
		],
		"*.{js,jsx,ts,tsx}": [
			"npm run format",
			"npm run lint"
		]
	}
}
