{
	"name": "my-github-page",
	"description": "My home page",
	"dependencies": {
		"date-fns": "2.16.1",
		"sirv-cli": "1.0.8"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "16.0.0",
		"@rollup/plugin-image": "2.0.5",
		"@rollup/plugin-inject": "4.0.2",
		"@rollup/plugin-json": "4.1.0",
		"@rollup/plugin-node-resolve": "10.0.0",
		"@rollup/plugin-typescript": "6.1.0",
		"@tsconfig/svelte": "2.0.1",
		"@typescript-eslint/eslint-plugin": "4.29.2",
		"@typescript-eslint/parser": "4.29.2",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-svelte3": "3.2.0",
		"husky": "4.3.0",
		"lint-staged": "10.5.1",
		"metascraper": "5.14.20",
		"metascraper-author": "5.14.20",
		"metascraper-date": "5.14.20",
		"metascraper-description": "5.14.20",
		"metascraper-image": "5.14.20",
		"metascraper-logo": "5.14.20",
		"metascraper-publisher": "5.14.20",
		"metascraper-title": "5.14.20",
		"metascraper-url": "5.14.20",
		"prettier": "2.3.2",
		"prettier-plugin-svelte": "2.3.1",
		"rollup": "2.33.1",
		"rollup-plugin-livereload": "2.0.0",
		"rollup-plugin-postcss": "3.1.8",
		"rollup-plugin-svelte": "6.1.0",
		"rollup-plugin-terser": "7.0.2",
		"sass": "1.38.0",
		"svelte": "3.42.2",
		"svelte-check": "1.3.0",
		"svelte-material-ui": "1.0.0",
		"svelte-preprocess": "4.7.4",
		"svelte-spa-router": "3.2.0",
		"tslib": "2.3.1",
		"typescript": "4.3.5"
	},
	"scripts": {
		"meta": "node links.js",
		"format": "prettier --write",
		"format:check": "prettier --check .",
		"lint": "eslint",
		"lint:all": "eslint .",
		"build": "rollup -c",
		"start": "sirv public --single",
		"dev": "rollup -c -w",
		"validate": "svelte-check"
	},
	"keywords": [
		"svelte",
		"typescript",
		"webpack",
		"sass"
	],
	"author": "n0th1ng-else",
	"license": "MIT",
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{md,yml,scss,css,svelte}": [
			"npm run format"
		],
		"*.{js,jsx,ts,tsx}": [
			"npm run format",
			"npm run lint"
		]
	}
}
