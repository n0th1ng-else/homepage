{
	"name": "my-github-page",
	"version": "0.0.0-semantically-released",
	"description": "My home page",
	"engines": {
		"node": ">=20.14.0"
	},
	"dependencies": {
		"date-fns": "3.6.0",
		"highlight.js": "11.9.0",
		"js-cookie": "3.0.5",
		"marked": "12.0.1",
		"marked-highlight": "2.1.1",
		"parse-md": "3.0.3",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "5.0.1",
		"@sveltejs/adapter-static": "3.0.1",
		"@sveltejs/kit": "2.5.5",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@tsconfig/svelte": "5.0.4",
		"@types/js-cookie": "3.0.6",
		"@types/node": "20.12.5",
		"@types/slug": "5.0.8",
		"@typescript-eslint/eslint-plugin": "7.5.0",
		"@typescript-eslint/parser": "7.5.0",
		"cross-spawn": "7.0.3",
		"dotenv-cli": "7.4.1",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"lint-staged": "15.2.2",
		"metascraper": "5.45.3",
		"metascraper-author": "5.45.0",
		"metascraper-date": "5.45.0",
		"metascraper-description": "5.45.0",
		"metascraper-image": "5.45.0",
		"metascraper-logo": "5.45.0",
		"metascraper-publisher": "5.45.0",
		"metascraper-title": "5.45.0",
		"metascraper-url": "5.45.0",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.2",
		"sass": "1.74.1",
		"semantic-release": "23.0.7",
		"slug": "9.0.0",
		"svelte": "4.2.12",
		"svelte-check": "3.6.9",
		"tslib": "2.6.2",
		"typescript": "5.4.4",
		"vite": "5.2.8"
	},
	"scripts": {
		"meta": "node src/ci/links.js",
		"meta:dev": "dotenv npm run meta",
		"version": "node src/ci/version.js",
		"format": "prettier --write ./**/*.svelte && prettier --write .",
		"format:check": "prettier --plugin-search-dir --check .",
		"lint": "eslint .",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv vite dev",
		"preview": "vite preview",
		"server": "node dist",
		"prepare": "svelte-kit sync",
		"draft": "node src/ci/create-md.js",
		"welcome": "node src/ci/welcome.js",
		"start": "npm run welcome && npm run server"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"typescript",
		"webpack",
		"sass"
	],
	"author": "Sergey Nikitin <srg.post@gmail.com>",
	"license": "MIT",
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{md,yml,scss,css,svelte}": [
			"npm run format"
		],
		"*.{js,jsx,ts,tsx}": [
			"npm run format",
			"npm run lint"
		]
	}
}
