{
	"name": "my-github-page",
	"version": "0.0.0-semantically-released",
	"description": "My home page",
	"engines": {
		"node": ">=20.10.0",
		"pnpm": ">=9"
	},
	"dependencies": {
		"date-fns": "3.6.0",
		"highlight.js": "11.10.0",
		"js-cookie": "3.0.5",
		"marked": "14.1.0",
		"marked-highlight": "2.1.4",
		"parse-md": "3.0.3",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "5.2.2",
		"@sveltejs/adapter-static": "3.0.4",
		"@sveltejs/kit": "2.5.25",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@tsconfig/svelte": "5.0.4",
		"@types/js-cookie": "3.0.6",
		"@types/node": "22.5.1",
		"@types/slug": "5.0.9",
		"@typescript-eslint/eslint-plugin": "7.15.0",
		"@typescript-eslint/parser": "7.15.0",
		"cross-spawn": "7.0.3",
		"dotenv-cli": "7.4.2",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.43.0",
		"lint-staged": "15.2.9",
		"metascraper": "5.45.23",
		"metascraper-author": "5.45.22",
		"metascraper-date": "5.45.22",
		"metascraper-description": "5.45.22",
		"metascraper-image": "5.45.22",
		"metascraper-logo": "5.45.22",
		"metascraper-publisher": "5.45.22",
		"metascraper-title": "5.45.22",
		"metascraper-url": "5.45.22",
		"postcss": "8.4.41",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.6",
		"sass": "1.77.8",
		"semantic-release": "24.1.0",
		"slug": "9.1.0",
		"svelte": "4.2.19",
		"svelte-check": "3.8.6",
		"tslib": "2.7.0",
		"typescript": "5.5.4",
		"vite": "5.4.2"
	},
	"scripts": {
		"meta": "node src/ci/links.js",
		"meta:dev": "dotenv pnpm meta",
		"version": "node src/ci/version.js",
		"format": "prettier --write ./**/*.svelte && prettier --write .",
		"format:check": "prettier --plugin-search-dir --check .",
		"lint": "eslint .",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv vite dev",
		"preview": "vite preview",
		"server": "node dist",
		"prepare": "svelte-kit sync",
		"draft": "node src/ci/create-md.js",
		"welcome": "node src/ci/welcome.js",
		"start": "pnpm run welcome && pnpm run server"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"typescript",
		"webpack",
		"sass"
	],
	"author": "Sergey Nikitin <srg.post@gmail.com>",
	"license": "MIT",
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{md,yml,scss,css,svelte}": [
			"pnpm format"
		],
		"*.{js,jsx,ts,tsx}": [
			"pnpm format",
			"pnpm lint"
		]
	}
}
